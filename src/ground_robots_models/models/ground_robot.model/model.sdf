<?xml version="1.0"?>
<sdf version="1.5">
  <model name="ground_robot">
    <static>false</static>

    <!-- ********************** ROBOT BASE *********************************  -->
    <link name="base_link">
      <pose>0 0 0.25 0 0 0</pose>
      <inertial>
        <inertia>
          <ixx>0.282625</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.282625</iyy>
          <iyz>0.0</iyz>
          <izz>0.415625</izz>
        </inertia>
        <mass>10</mass>
      </inertial>

      <collision name="base_collision">
        <geometry>
          <mesh>
            <uri>model://ground_robot.model/meshes/base.stl</uri>
          </mesh>
        </geometry>
      </collision>

      <visual name="base_visual">
        <geometry>
          <box>
            <size>0.5 0.5 0.3</size>
          </box>
        </geometry>
        <material>
          <ambient>1.0 1.0 1.0 1.0</ambient>
          <diffuse>1.0 1.0 1.0 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
          <emissive>0.5 0.5 0.5 0.5</emissive>
        </material>
      </visual>
    </link>

    <!-- ****************************** LIDAR *****************************    -->
    <link name="lidar_link">
      <inertial>
        <pose>0 0 0.42 0 0 0</pose>
        <inertia>
          <ixx>0.0001325</ixx>
          <ixy>0.000</ixy>
          <ixz>0.000</ixz>
          <iyy>0.0001325</iyy>
          <iyz>0.000</iyz>
          <izz>0.0001915</izz>
        </inertia>
        <mass>0.190</mass>
      </inertial>

      <collision name="lidar_collision">
        <pose>0 0 0.42 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.0375</radius>
            <length>0.04</length>
          </cylinder>
        </geometry>
      </collision>

      <visual name="lidar_visual">
        <pose>0 0 0.42 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.0375</radius>
            <length>0.04</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.0 1.0</ambient>
          <diffuse>0.0 0.0 0.0 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
          <emissive>0.0 0.0 0.0 1.0</emissive>
        </material>
      </visual>

      <sensor name="lidar" type="ray">
        <pose>0 0 0.41 0 0 0</pose>
        <always_on>true</always_on>
        <visualize>false</visualize>
        <update_rate>5</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>2000</samples>
              <resolution>0.25</resolution>
              <min_angle>-3.14159</min_angle>
              <max_angle>3.14159</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.15</min>
            <max>6.0</max>
            <resolution>0.095</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <plugin name="scan" filename="libgazebo_ros_ray_sensor.so">
          <ros>
            <remapping>~/out:=scan</remapping>
          </ros>
          <output_type>sensor_msgs/LaserScan</output_type>
          <frame_name>lidar_link</frame_name>
        </plugin>
      </sensor>
    </link>

    <!-- *********************** DRIVE WHEELS ******************************  -->
    <link name="drivewhl_l_link">
      <pose>-0.075 0.2 0.125 0 1.570796 1.570796</pose>

      <inertial>
        <pose>0 0 0 1.570796 0 0</pose>
        <inertia>
          <ixx>0.009178125</ixx>
          <ixy>0.000</ixy>
          <ixz>0.000</ixz>
          <iyy>0.0177465</iyy>
          <iyz>0.000</iyz>
          <izz>0.009178125</izz>
        </inertia>
        <mass>2.0</mass>
      </inertial>

      <collision name="drivewhl_l_collision">
        <geometry>
          <cylinder>
            <radius>0.125</radius>
            <length>0.04</length>
          </cylinder>
        </geometry>
      </collision>

      <visual name="drivewhl_l_visual">
        <geometry>
          <cylinder>
            <radius>0.125</radius>
            <length>0.04</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.0 1.0</ambient>
          <diffuse>0.0 0.0 0.0 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
          <emissive>0.0 0.0 0.0 1.0</emissive>
        </material>
      </visual>
    </link>

    <link name="drivewhl_r_link">
      <pose>-0.075 -0.2 0.125 0 1.570796 1.570796</pose>

      <inertial>
        <pose>0 0 0 1.570796 0 0 </pose>
        <inertia>
          <ixx>0.009178125</ixx>
          <ixy>0.000</ixy>
          <ixz>0.000</ixz>
          <iyy>0.0177465</iyy>
          <iyz>0.000</iyz>
          <izz>0.009178125</izz>
        </inertia>
        <mass>2.0</mass>
      </inertial>

      <collision name="drivewhl_r_collision">
        <geometry>
          <cylinder>
            <radius>0.125</radius>
            <length>0.04</length>
          </cylinder>
        </geometry>
      </collision>

      <visual name="drivewhl_r_visual">
        <geometry>
          <cylinder>
            <radius>0.125</radius>
            <length>0.04</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.0 1.0</ambient>
          <diffuse>0.0 0.0 0.0 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
          <emissive>0.0 0.0 0.0 1.0</emissive>
        </material>
      </visual>
    </link>

    <!-- *********************** CASTER WHEEL ******************************  -->
    <link name='front_caster'>
      <pose>0.175 0 0.05 1.570796 0 0</pose>

      <inertial>
        <pose>0 0 0 -1.570796 0 0</pose>
        <inertia>
          <ixx>0.000410</ixx>
          <ixy>0.000</ixy>
          <ixz>0.000</ixz>
          <iyy>0.000757</iyy>
          <iyz>0.000</iyz>
          <izz>0.000410</izz>
        </inertia>
        <mass>0.5</mass>
      </inertial>

      <collision name='front_caster_collision'>
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.025</length>
          </cylinder>
        </geometry>
      </collision>
      
      <visual name="front_caster_visual">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.025</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.0 1.0</ambient>
          <diffuse>0.0 0.0 0.0 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
          <emissive>0.0 0.0 0.0 1.0</emissive>
        </material>
      </visual>
    </link>

    <!-- ************************ JOINTS ***********************************  -->
    <!-- Pose of the joint is the same as the child link frame -->
    <!-- Axis is the axis of rotation relative to the child link frame -->


    <joint name="drivewhl_l_joint" type="revolute">
      <parent>base_link</parent>
      <child>drivewhl_l_link</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <joint name="drivewhl_r_joint" type="revolute">
      <parent>base_link</parent>
      <child>drivewhl_r_link</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <joint name='caster_joint' type='fixed'>
      <parent>base_link</parent>
      <child>front_caster</child>
      <pose>0 0 0 0 0 0</pose>
    </joint>

    <joint name="lidar_joint" type="fixed">
      <parent>base_link</parent>
      <child>lidar_link</child>
      <pose>0 0 0 0 0 0</pose>
    </joint>

    <!-- *********************** WHEEL ODOMETRY ***************************    -->
    <plugin name="ground_robot_diff_drive" filename="libgazebo_ros_diff_drive.so">

      <update_rate>30</update_rate>

      <!-- wheels -->
      <left_joint>drivewhl_l_joint</left_joint>
      <right_joint>drivewhl_r_joint</right_joint>

      <!-- kinematics -->
      <wheel_separation>0.46</wheel_separation>
      <wheel_diameter>0.28</wheel_diameter>

      <!-- limits -->
      <max_wheel_torque>20</max_wheel_torque>
      <max_wheel_acceleration>1.0</max_wheel_acceleration>

      <!-- Receive velocity commands on this ROS topic -->
      <command_topic>cmd_vel</command_topic>

      <!-- output -->
      <!-- When false, publish no wheel odometry data to a ROS topic -->
      <publish_odom>true</publish_odom>

      <!-- When true, publish coordinate transform from odom to base_footprint -->
      <!-- I usually use the robot_localization package to publish this transform -->
      <publish_odom_tf>true</publish_odom_tf>

      <!-- When true, publish coordinate transform from base_link to the wheels -->
      <!-- The robot_state_publisher package is often used to publish this transform -->
      <publish_wheel_tf>false</publish_wheel_tf>

      <odometry_topic>odom</odometry_topic>
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>

      <!-- Odometry source, 0 for ENCODER, 1 for WORLD, defaults to WORLD -->
      <odometry_source>1</odometry_source>

      <!-- Change the ROS topic we will publish the odometry data to -->
      <ros>
        <remapping>odom:=wheel/odometry</remapping>
      </ros>

    </plugin>

    <!-- *********************** JOINT STATE PUBLISHER *********************   -->

    <plugin name="g_robot_joint_state" filename="libgazebo_ros_joint_state_publisher.so">
      <ros>
        <remapping>~/out:=joint_states</remapping>
      </ros>
      <update_rate>30</update_rate>
      <joint_name>drivewhl_l_joint</joint_name>
      <joint_name>drivewhl_r_joint</joint_name>
    </plugin>

  </model>
</sdf>