<?xml version="1.0"?>
<sdf version="1.6">
    <model name='GRobot'>
        <pose>0 0 0 0 0 0</pose>

        <link name='robot_base'>
            <inertial>
                <mass>10</mass>
                <pose>0 0 0.25</pose>
                <inertia>
                    <ixx>0.002125</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.002125</iyy>
                    <iyz>0</iyz>
                    <izz>0.003125</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>model://robot.model/meshes/robot_base.dae</uri>
                        <scale>1 1 1</scale>
                    </mesh>
                </geometry>
            </visual>
            <collision name='collision'>
                <geometry>
                    <mesh>
                        <uri>model://robot.model/meshes/robot_base.dae</uri>
                        <scale>1 1 1</scale>
                    </mesh>
                </geometry>
            </collision>
        </link>

